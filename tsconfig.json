    "compilerOptions": {
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "~/*": ["./*"]
        },
        "outDir": "./dist"
    },
    "exclude": ["migrate.ts"]
}
